
import SynPSG.PDF

subroutine AttachFileTest
    record
        pdf, @PdfFile
    endrecord
proc

    ;;Create a PDF document and attach an existing text file
    pdf = new PdfFile("DBLDIR:REL_DBL.TXT",,,,,,,,,"OUTPUT:")
    pdf.FileName = "AttachFile.pdf"
    pdf.PageLayout = PdfPageLayout.Single

    ;;Show the results
    pdf.Preview()

    xreturn

endsubroutine

