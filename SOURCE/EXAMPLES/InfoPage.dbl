
import SynPSG.PDF

subroutine InfoPage
    record
        pdf, @PdfFile
    endrecord
proc
    ;;Create a new PDF document
    pdf = new PdfFile(,,,,,,,,,,,"OUTPUT:")

    ;;Override the default file to prevent the date/time stamp prefix.
    pdf.FileName = "InfoPage.pdf"

    ;;Add an information page
    pdf.AddInfoPage()

    ;;Save the document and show the results
    pdf.Preview()

    xreturn

endsubroutine

